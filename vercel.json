{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/blog/(.*)",
      "destination": "/apps/blog/dist/blog/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/apps/web/dist/$1"
    }
  ]
}